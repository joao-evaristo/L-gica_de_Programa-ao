#include <stdio.h>
#include <math.h>
int main(){
  double A, B, C,
  triangle, circle, trapeze, square, rectangle;
  scanf("%lf%lf%lf", &A, &B, &C);
  triangle = (A * C)/2;
  circle = 3.14159 * pow(C, 2);
  trapeze = ((A+B)*C)/2 ;
  square = B * B;
  rectangle = A * B;
  printf("TRIANGULO: %.3lf\nCIRCULO: %.3lf\nTRAPEZIO: %.3lf\nQUADRADO: %.3lf\nRETANGULO: %.3lf\n", triangle, circle, trapeze, square, rectangle);
  return  0;
}
